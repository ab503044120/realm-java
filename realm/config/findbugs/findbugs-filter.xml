<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!--
    Please prefer annotations in the code, to ignore Findbugs issues
    -->

    <Match>
        <!-- ignore all issues in resource generation -->
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>

    <!-- Unit tests
        As of Jan 6, 2017, Unit Test code will not pass Findbugs.
        Clean the build directory and build only the product code.
    -->
</FindBugsFilter>
